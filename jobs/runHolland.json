{
    "jobType": "batch",

    "stationsFilepath": "data/StationsHolland.csv",
    "connectionsFilepath": "data/ConnectiesHolland.csv",

    "runs": 10,

    "algorithm": "annealing",

    "runName": "hollandOpt",
    "targetFolder": "results/hollandOpt",
    "maxRoutes": 7,
    "maxDuration": 120,

    "stepFunction": "route",
    "coolingScheme": "linear",

    "convergenceLimit": 5000,
    "randomIterations": 1000,

    "initialTemperature": 64,
    "coolingConstant": 0.0064
}