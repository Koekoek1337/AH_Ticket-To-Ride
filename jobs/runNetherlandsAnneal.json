{
    "jobType": "batch",

    "stationsFilepath": "data/StationsNationaal.csv",
    "connectionsFilepath": "data/ConnectiesNationaal.csv",

    "algorithm": "annealing",
    "stepFunction": "route",
    "coolingScheme": "logarithmic",

    "initialTemperature": 50,
    "coolingConstant": 125,
    
    "runName": "annealLogc250",
    "targetFolder": "results/Netherlands/annealLog",
    "exportImprovements": false,

    "maxRoutes": 20,
    "maxDuration": 180,
    
    "convergenceLimit": 5000,
    "randomIterations": 1000,
    
    "recordAll": false
}